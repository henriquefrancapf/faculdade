

if __name__== '__main__':

        n= int(input('digite numero desejado:'))
        
        
        fn1 = 0 
        fn2 = 1

        for i in range (n) : 
         resultado = fn1 + fn2 
         fn1 = fn2  
         fn2 = fn1 + fn2 
         

         print (F'resultado: { resultado}  | fn1 =>{fn1}|fn2 =>{fn2}')
