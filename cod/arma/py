from random import randint
espada_ataq= randint(30,120)
espada_dura= randint(40,80)

arco_ataq= randint(20,90)
arco_dura= randint(80,150)

lanca_ataq= randint(70,100)
lanca_dura= randint(30,120)

if espada_ataq > 50 and espada_dura >70:
    print('espada')
elif arco_ataq > 50 and arco_dura > 70:
    print('arco')
elif lanca_ataq > 50 and lanca_dura > 70:
    print('lanca')
else:
    print ('escolha outra arma!')
